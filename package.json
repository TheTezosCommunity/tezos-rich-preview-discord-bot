{
    "name": "discord-tezos-previews",
    "version": "1.0.0",
    "description": "A Discord bot that provides rich NFT previews for Tezos marketplace links",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "bun run dist/index.js",
        "dev": "bun run src/index.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "discord",
        "bot",
        "tezos",
        "nft",
        "preview",
        "objkt"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "discord.js": "^14.14.1",
        "dotenv": "^16.3.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/node": "^20.10.5",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "typescript": "^5.3.3"
    },
    "engines": {
        "bun": ">=1.0.0"
    }
}
